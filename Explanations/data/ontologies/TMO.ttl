_:genid43 rdf:type owl:Class ;
           owl:complementOf SOMA:NonmanifestedSituation .

[ rdf:type owl:Axiom ;
   owl:annotatedSource SOMA:hasTerminalSituation ;
   owl:annotatedProperty rdfs:range ;
   owl:annotatedTarget _:genid43 ;
   rdfs:comment "If something is a terminal situation then it must be a manifested situation (regardless of whether it was expected or not)."
 ] .



###  http://www.ease-crc.org/ont/SOMA.owl#BodyMovement
SOMA:BodyMovement rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Motion ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                        owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAgent>
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                        owl:someValuesFrom [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                                                                                                  [ rdf:type owl:Restriction ;
                                                                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf> ;
                                                                                                                    owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAgent>
                                                                                                                  ]
                                                                                                                ) ;
                                                                                             rdf:type owl:Class
                                                                                           ]
                                                                      ]
                                                                    )
                                                      ]
                                  ] ;
                  rdfs:comment """Motion described in terms of how parts of an Agent's body move.

As such, this concept can be applied only to motions involving some PhysicalAgent, or body parts of a PhysicalAgent.""" ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Boiling
SOMA:Boiling rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Vaporizing ;
             rdfs:comment "In each instance of this collection some liquid matter is raised to its boilingPoint and is thereby changed from being in the Liquid-StateOfMatter to being in the Gaseous-StateOfMatter." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#BoxShape
SOMA:BoxShape rdf:type owl:Class ;
              rdfs:subClassOf SOMA:ShapeRegion ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:hasHeight ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:float
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:hasLength ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:float
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:hasWidth ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:float
                              ] ;
              rdfs:comment "A symmetrical shape, either solid or hollow, contained by six rectangles." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
              rdfs:label "Box shape" .


###  http://www.ease-crc.org/ont/SOMA.owl#CanCut
SOMA:CanCut rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Variability ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty SOMA:affordsBearer ;
                              owl:allValuesFrom SOMA:Cutter
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty SOMA:affordsTrigger ;
                              owl:allValuesFrom SOMA:CutObject
                            ] ;
            rdfs:comment "The disposition of an object (the tool) to cut other objects. Such as a knife has cutting ability to cut a cucumber into pieces." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Capability
SOMA:Capability rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Disposition ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy> ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass [ owl:intersectionOf ( SOMA:Affordance
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty SOMA:definesBearer ;
                                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                       owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role>
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty SOMA:definesTrigger ;
                                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                       owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role>
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask> ;
                                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                       owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task>
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ]
                                ] ;
                rdfs:comment "Capability" ,
                             "The disposition of an agent to bring about certain effects, and to achieve certain goals." ,
                             "The tendency of an object (the bearer) to be able to perform certain tasks together with others (the triggers) and in which the Bearer is the executor of the associated Task and will therefore usually be an Agent." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Capacity
SOMA:Capacity rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Intrinsic ;
              rdfs:comment "The maximum amount an object can contain." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Catching
SOMA:Catching rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Actuating ;
              owl:disjointWith SOMA:PickingUp ;
              rdfs:comment "A task by which an Agent stops a moving object and gains kinematic control over it, usually by grasping." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CausalEventRole
SOMA:CausalEventRole rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:CausativeRole ;
                     rdfs:comment """A role filled by a description of some action or process that brings about a motion.

As an example, consider the utterance \"the tennisman served the ball by hitting it with the racket.\" In this utterance, the filler of the CausalEventRole is expressed by the \"by hitting it with the racket\" constituent.""" ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Causal event role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CausativeRole
SOMA:CausativeRole rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:EventAdjacentRole ;
                   rdfs:comment """A role classifying objects that are responsible in bringing about an event.

The paradigmatic example is the Agent performing an Action -- the Agent is the effective cause of the Action it performs. However, not all objects causing events are agents.""" ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Causative role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CausedMotionTheory
SOMA:CausedMotionTheory rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:ImageSchemaTheory ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                          owl:someValuesFrom SOMA:CausalEventRole
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                          owl:someValuesFrom SOMA:Instrument
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                          owl:someValuesFrom SOMA:Patient
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                          owl:someValuesFrom SOMA:PerformerRole
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart> ;
                                          owl:someValuesFrom SOMA:SourcePathGoalTheory
                                        ] ;
                        rdfs:comment """A schematic theory describing a situation in which an agent performs an action which causes an object to move along a certain path. A popular example being \"He sneezed the napkin off the table.\" (Goldberg 1995)

Let xA, xP be objects filling the agent, patient roles of this schema. Then one can infer that xA movesObject xP.""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                        rdfs:label "Caused motion theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Channel
SOMA:Channel rdf:type owl:Class ;
             rdfs:subClassOf SOMA:PathRole ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTask> ;
                               owl:someValuesFrom SOMA:CommunicationTask
                             ] ;
             rdfs:comment "A Channel in a Communication Task is the path of travel by a Message, e.g., via WLAN, air (in the case of a Message classifying soundwaves) or a telephone cable."@en ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
             rdfs:label "Channel"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CheckingObjectPresence
SOMA:CheckingObjectPresence rdf:type owl:Class ;
                            rdfs:subClassOf SOMA:Perceiving ;
                            rdfs:comment "A task by which an Agent uses its sensors to check for the presence of a specific object and to obtain some other information about it, e.g. pose." ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                            rdfs:label "Checking object presence"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ChemicalProcess
SOMA:ChemicalProcess rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process> ;
                     rdfs:comment """A process by which the chemical constituency of an Entity or set of Entities changes.

In some sense any process that results in entities being created or destroyed might trivially fit here, however this concept is intended specifically for Events where the occuring chemical reactions are of importance.""" ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                     rdfs:label "Chemical process" .


###  http://www.ease-crc.org/ont/SOMA.owl#Choice
SOMA:Choice rdf:type owl:Class ;
            rdfs:subClassOf SOMA:ResultRole ;
            rdfs:comment "The output of a, e.g, Selecting Task."@en ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-AGENT.owl> ;
            rdfs:label "Choice"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CircularCylinder
SOMA:CircularCylinder rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:CylinderShape ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty SOMA:hasRadius ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange xsd:float
                                      ] ;
                      rdfs:comment "A cylinder figure with circular cross section." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                      rdfs:label "Circular cylinder" .


###  http://www.ease-crc.org/ont/SOMA.owl#Classifier
SOMA:Classifier rdf:type owl:Class ;
                rdfs:subClassOf SOMA:StatisticalReasoner ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                rdfs:label "Classifier"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ClausalObject
SOMA:ClausalObject rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Phrase ;
                   rdfs:comment "A clause is a phrase containing a subject and a predicate." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                   rdfs:label "Clausal object" .


###  http://www.ease-crc.org/ont/SOMA.owl#Clean
SOMA:Clean rdf:type owl:Class ;
           rdfs:subClassOf SOMA:CleanlinessRegion ;
           rdfs:comment "A cleanliness region with values considered as clean." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cleaning
SOMA:Cleaning rdf:type owl:Class ;
              rdfs:subClassOf SOMA:ModifyingPhysicalObject ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:isTaskAffordedBy ;
                                owl:someValuesFrom SOMA:Purification
                              ] ;
              rdfs:comment "This task in which an agent restores all the objects to their destined locations, wiping a specific object" ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cleanliness
SOMA:Cleanliness rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:SocialQuality ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                   owl:someValuesFrom SOMA:CleanlinessRegion
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                   owl:allValuesFrom SOMA:CleanlinessRegion
                                 ] ;
                 rdfs:comment "The quality of being clean." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CleanlinessRegion
SOMA:CleanlinessRegion rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region> ;
                       rdfs:comment "Encodes the cleanliness of an object." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Cleanliness region"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Client-Server_Specification
SOMA:Client-Server_Specification rdf:type owl:Class ;
                                 rdfs:subClassOf SOMA:InterfaceSpecification ,
                                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole> ;
                                                                          owl:someValuesFrom SOMA:ClientRole
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole> ;
                                                                          owl:someValuesFrom SOMA:ServerRole
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ] ;
                                 rdfs:comment """An API Secification that describes the well known Client-Server pattern:

The Client-server model is a distributed application structure that partitions tasks or workloads between the providers of a resource or service, called servers, and service requesters, called clients Often clients and servers communicate over a computer network on separate hardware, but both client and server may reside in the same system. A server host runs one or more server programs, which share their resources with clients. A client usually does not share any of its resources, but it requests content or service from a server. Clients, therefore, initiate communication sessions with servers, which await incoming requests. Examples of computer applications that use the client-server model are email, network printing, and the World Wide Web.

Source: https://en.wikipedia.org/wiki/Client%E2%80%93server_model"""@en ;
                                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                                 rdfs:label "Client-Server Specification"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ClientRole
SOMA:ClientRole rdf:type owl:Class ;
                rdfs:subClassOf SOMA:InterfaceComponentRole ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn> ;
                                  owl:someValuesFrom SOMA:Client-Server_Specification
                                ] ;
                rdfs:comment """The Client-server model is a distributed application structure that partitions tasks or workloads between the providers of a resource or service, called servers, and service requesters, called clients Often clients and servers communicate over a computer network on separate hardware, but both client and server may reside in the same system. A server host runs one or more server programs, which share their resources with clients. A client usually does not share any of its resources, but it requests content or service from a server. Clients, therefore, initiate communication sessions with servers, which await incoming requests. Examples of computer applications that use the client-server model are email, network printing, and the World Wide Web.

Source: https://en.wikipedia.org/wiki/Client%E2%80%93server_model"""@en ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                rdfs:label "Client role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Closing
SOMA:Closing rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Actuating ;
             owl:disjointWith SOMA:Delivering ,
                              SOMA:Fetching ,
                              SOMA:Lifting ,
                              SOMA:Opening ,
                              SOMA:Pulling ,
                              SOMA:Pushing ,
                              SOMA:Squeezing ;
             rdfs:comment "A task in which an Agent manipulates a container so as to block access to its interior." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Clumsiness
SOMA:Clumsiness rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Amateurish ;
                rdfs:comment "A description of clumsy behavior." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CognitiveAgent
SOMA:CognitiveAgent rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                    owl:disjointWith SOMA:SubCognitiveAgent ;
                    rdfs:comment "An agent that is capable to act on its own, in contrast to sub-cognitive Agents, that need to have their intentionality bestowed upon some other agent."@en ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                    rdfs:label "cognitive agent"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Collision
SOMA:Collision rdf:type owl:Class ;
               rdfs:subClassOf SOMA:ProcessType ;
               rdfs:comment "A contact event of objects bumping into each other such that their movement is affected." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Color
SOMA:Color rdf:type owl:Class ;
           rdfs:subClassOf SOMA:Extrinsic ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                             owl:someValuesFrom SOMA:ColorRegion
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                             owl:allValuesFrom SOMA:ColorRegion
                           ] ;
           rdfs:comment "The color of an object. Color regions encode the color value in some space such as RGB or HSV, and may further be used to classify the color as red, dark, etc. The color of an object may have different facets, e.g. a red and blue color." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
           rdfs:label "Color" .


###  http://www.ease-crc.org/ont/SOMA.owl#ColorRegion
SOMA:ColorRegion rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor> ;
                                   owl:allValuesFrom SOMA:Color
                                 ] ;
                 rdfs:comment "Encodes the color of an object." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                 rdfs:label "Color region" .


###  http://www.ease-crc.org/ont/SOMA.owl#CommandingTask
SOMA:CommandingTask rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:IllocutionaryTask ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                      owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                          owl:someValuesFrom SOMA:ImperativeClause
                                                        ]
                                    ] ;
                    rdfs:comment "An Illocutionary act where the Sender emits some Message with the intent to cause the Receiver to perform some action."@en ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                    rdfs:label "Commanding task"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommitedObject
SOMA:CommitedObject rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:ConnectedObject ;
                    rdfs:comment "An object committed to a bigger whole. After being committed, the object does not exist anymore in its old form." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Commited object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommunicationAction
SOMA:CommunicationAction rdf:type owl:Class ;
                         rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action> ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                           owl:someValuesFrom SOMA:LinguisticObject
                                         ] ;
                         rdfs:comment "An action in which an Agent uses some actuator for communication purposes."@en ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                         rdfs:label "Locutionary action"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommunicationReport
SOMA:CommunicationReport rdf:type owl:Class ;
                         rdfs:subClassOf SOMA:CommunicationTask ;
                         rdfs:comment "A task in which an Agent endeavors to describe truthfully some state of affairs." ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                         rdfs:label "Communication report"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommunicationTask
SOMA:CommunicationTask rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ,
                                       [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Channel
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Message
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Receiver
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Sender
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                         owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                 owl:unionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                                               <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject>
                                                                                             )
                                                                               ]
                                                           ]
                                       ] ;
                       rdfs:comment """A Task in which two or more Agents exchange information. A CommunicationTask classifies only Events that have only Agents and Social objects as participants.

Of course, the means of exchange is Physical, however this concept is to classify events for which we are not interested in the physical substrate, but rather who communicated and what the information content was.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                       rdfs:label "Communication task"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommunicationTopic
SOMA:CommunicationTopic rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:ResourceRole ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                          owl:allValuesFrom [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject>
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn> ;
                                                                                     owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy> ;
                                                                                                          owl:someValuesFrom SOMA:CommunicationTask
                                                                                                        ]
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ]
                                        ] ;
                        rdfs:comment """A role that appears in communication tasks, and indicates what the communication is about.

CommunicationTopic can only classify a Social object that participates in an Action that is classified as (the execution of) a CommunicationTask.

Note that classifies here is used in the plays-role sense. This isn't to say that the social object, ie the information exchanged in the communication, is an instance of the topic. Rather, the topic role refers to what the information is about.

For example, if the topic of a communication is flowers, this does not mean the words themselves are flowers, merely that, in some sense, they are about flowers.""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                        rdfs:label "Communication topic"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Composing
SOMA:Composing rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Variability ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:affordsTrigger ;
                                 owl:allValuesFrom SOMA:ConnectedObject
                               ] ;
               rdfs:comment "The disposition of an object (the tool) to change the compositional structure of others." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Computer_Language
SOMA:Computer_Language rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:FormalLanguage ;
                       rdfs:comment """A computer language is a formal language used in communication with a computer.

From Wikipedia: https://en.wikipedia.org/wiki/Computer_language"""@en ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                       rdfs:label "Computer Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Computer_Program
SOMA:Computer_Program rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( SOMA:Structured_Text
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty SOMA:isGivenMeaningBy ;
                                                                   owl:someValuesFrom SOMA:Programming_Language
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ,
                                          [ owl:intersectionOf ( SOMA:Structured_Text
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses> ;
                                                                   owl:someValuesFrom SOMA:Algorithm
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:comment "The Program itself (the specific set of instruction in a Programming Language), not the file that it is contained in nor the implemented algorithm!"@en ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                      rdfs:label "Computer program"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Conclusion
SOMA:Conclusion rdf:type owl:Class ;
                rdfs:subClassOf SOMA:CreatedObject ,
                                SOMA:Knowledge ;
                rdfs:comment "An object that is derived from some premise using some inference rules." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                rdfs:label "Conclusions" .


###  http://www.ease-crc.org/ont/SOMA.owl#ConditionalSuccedence
SOMA:ConditionalSuccedence rdf:type owl:Class ;
                           rdfs:subClassOf SOMA:Succedence ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty SOMA:hasBinding ;
                                             owl:allValuesFrom SOMA:CounterfactualBinding
                                           ] ;
                           rdfs:comment """A relation that holds between two OGPTasks that belong to the same OGPWorkflow, and which means that, if a condition is met, the successor task invocation is to be executed after the predecessor task invocation completes.

The condition is a conjunction of CounterfactualBindings. These bindings may be RoleRoleBindings (meaning, test whether the fillers for these Roles/Parameters are the same) or RoleFillerBindings (meaning, test whether the filler of the Role unifies with the candidate Entity).

An empty conjunction of CounterfactualBindings is assumed to be True.""" ,
                                        "Note: in RobotWorkflows, Tasks typically may have several possible successors, to be selected based on condition-testing." ;
                           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-WF.owl> ;
                           rdfs:label "Conditional succedence"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Configuration
SOMA:Configuration rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes> ;
                                     owl:someValuesFrom SOMA:StateType
                                   ] ;
                   rdfs:comment """A description of a State. This includes e.g. what acceptable regions for participant objects of the State might look like.
A configuration of the world is construed to be stable on its own. Outside disturbances may cause state transitions, and the settling into some other, self-stable configuration.

Several other Description subclasses may function as Configurations. For example, a Goal is a description of a desired State. A Norm describes a State that should be maintained. A Diagnosis describes a State that causes certain symptoms etc.

Note a couple of issues here. First one relates to what \"configuration\" means; in particular, this doesn't mean a configuration that is unchanging according to any viewpoint. The analogy here is the \"macrostate\" from thermodynamics: a macrostate with two gases mixed does not mean all gas particles are motionless, but rather that the locations and movements of gas particles are such that any particle is likely to have as many neighbors of one type as the other.

The second issue relates to what is \"outside\". The state is a configuration of some, but not necessarily all, Entities in the world. Entities not in this configuration are outside of it.""" ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ConnectedObject
SOMA:ConnectedObject rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Patient ;
                     rdfs:comment "An object that is combined with another object." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                     rdfs:label "Connected object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Connectivity
SOMA:Connectivity rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Disposition ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty SOMA:affordsBearer ;
                                    owl:allValuesFrom SOMA:ConnectedObject
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty SOMA:affordsTrigger ;
                                    owl:allValuesFrom SOMA:ConnectedObject
                                  ] ;
                  rdfs:comment "The disposition of an object (the connected object) to establish a connection with others." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Constructing
SOMA:Constructing rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:PhysicalTask ;
                  owl:disjointWith SOMA:ModifyingPhysicalObject ;
                  rdfs:comment "A task in which an Agent creates a new physical object."@en ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ContactState
SOMA:ContactState rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:StateType ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                    owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                                      ]
                                  ] ;
                  rdfs:comment "Classifies States in which some objects are in contact." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                  rdfs:label "Contact state"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Container
SOMA:Container rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Restrictor ;
               rdfs:comment "A role classifying an object used to contain others." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Containment
SOMA:Containment rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Disposition ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsBearer ;
                                   owl:allValuesFrom SOMA:Container
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsTrigger ;
                                   owl:allValuesFrom SOMA:IncludedObject
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:isDispositionOf ;
                                   owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
                                                       owl:someValuesFrom SOMA:Capacity
                                                     ]
                                 ] ;
                 rdfs:comment "Classifies States in which an object is placed inside another." ,
                              "The disposition of an object (the container) to contain others." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ContainmentState
SOMA:ContainmentState rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:FunctionalControl ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty SOMA:isStateTypeOf ;
                                        owl:someValuesFrom [ owl:intersectionOf ( SOMA:Container
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                                                                    owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                                                                        owl:onProperty SOMA:hasDisposition ;
                                                                                                        owl:someValuesFrom SOMA:Containment
                                                                                                      ]
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                      ] ;
                      rdfs:comment "Classifies States in which an object is kept inside another object." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                      rdfs:label "Containment state"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ContainmentTheory
SOMA:ContainmentTheory rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:ControlTheory ;
                       rdfs:comment """A schematic theory that describes a functional relation which ensures that the location of some entity, the locatum, is constrained to be within some region which is the interior of some other entity, the relatum.

This is also known as FunctionalControlInternal in GUM-4-space (Bateman et al. 2010).

Let xL, xR be objects filling the locatum, relatum roles of this schema. Then one can infer that xL isInsideOf xR.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                       rdfs:label "Containment theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ContinuousJoint
SOMA:ContinuousJoint rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:HingeJoint ;
                     rdfs:comment "A continuous hinge joint that rotates around an axis and has no upper and lower limits." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Continuous joint" .


###  http://www.ease-crc.org/ont/SOMA.owl#ControlTheory
SOMA:ControlTheory rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:FunctionalSpatialSchemaTheory ;
                   rdfs:comment """A description of functional-spatial configurations where one object controls another object's position in space, e.g. if a pot moves, then the popcorn contained therein moves, as well. Note that the objects do not need to be in direct contact.

Adopted from GUM-4-space (Bateman et al. 2010).""" ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                   rdfs:label "Control theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CounterfactualBinding
SOMA:CounterfactualBinding rdf:type owl:Class ;
                           rdfs:subClassOf SOMA:Binding ;
                           owl:disjointWith SOMA:FactualBinding ;
                           rdfs:comment """CounterfactualBindings are used to express conditions:

-- that two roles share a filler (RoleRoleBindings);
-- that the filler of a role is a particular entity (RoleFillerBindings). This is typically the case when the filler of the role may be one of a few constants, as is the case with the execution status of a robot task.

TODO: for the robot workflows we are likely to need at the start, testing equality of fillers is enough. In the future, we may want to replace that with unifiability of fillers.""" ;
                           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-WF.owl> ;
                           rdfs:label "Counterfactual binding"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Cover
SOMA:Cover rdf:type owl:Class ;
           rdfs:subClassOf SOMA:Barrier ;
           rdfs:comment "An object used to cover up others, such as a lid used as a cover for a pot." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Coverage
SOMA:Coverage rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Blockage ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:affordsBearer ;
                                owl:allValuesFrom SOMA:Cover
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:affordsTrigger ;
                                owl:allValuesFrom SOMA:CoveredObject
                              ] ;
              rdfs:comment "The disposition of an object (the cover) to hide or to protect objects by covering them. An example is a door that covers items in a container to e.g. prevent dust getting inside of the container." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CoverageTheory
SOMA:CoverageTheory rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:FunctionalSpatialSchemaTheory ;
                    rdfs:comment """A schematic theory of a functional relation between two objects such that one of them, the locatum, blocks access to the interior of the relatum.

Let xL, xR be objects filling the locatum, relatum roles of this schema. Then one can infer that xL coversObject xR.""" ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CoveredObject
SOMA:CoveredObject rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:BlockedObject ;
                   rdfs:comment "An object that is covered." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Covered object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CoveringTheory
SOMA:CoveringTheory rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:ExecutableSchematicTheory ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:someValuesFrom SOMA:Instrument
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass SOMA:Patient
                                    ] ;
                    rdfs:comment "A schematic theory of how an agent can use an instrument to prevent access to the interior of a patient." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                    rdfs:label "Covering theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CrackingTheory
SOMA:CrackingTheory rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:ExecutableSchematicTheory ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:someValuesFrom SOMA:Instrument
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:someValuesFrom SOMA:Patient
                                    ] ;
                    rdfs:comment "A schematic theory of how an agent can inflict damage to the surface of an object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                    rdfs:label "Cracking theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CreatedObject
SOMA:CreatedObject rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Patient ;
                   rdfs:comment "An object that is created." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                   rdfs:label "Created object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Creation
SOMA:Creation rdf:type owl:Class ;
              rdfs:subClassOf SOMA:ProcessType ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:isProcessTypeOf ;
                                owl:someValuesFrom SOMA:CreatedObject
                              ] ;
              rdfs:comment """A process by which an Entity is created in the physical world.

Note, most of the physical Entities we will be considering here are in fact arrangements of many other, smaller physical Entities. Therefore another way to look at this is, a 'Physical creation' is the process by which a set of physical Entities is arranged in a certain way, and the arrangement is then itself considered a physical Entity.""" ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
              rdfs:label "Creation" .


###  http://www.ease-crc.org/ont/SOMA.owl#CutObject
SOMA:CutObject rdf:type owl:Class ;
               rdfs:subClassOf SOMA:AlteredObject ;
               rdfs:comment "An object being cut down into pieces." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
               rdfs:label "Cut object" .


###  http://www.ease-crc.org/ont/SOMA.owl#Cuttability
SOMA:Cuttability rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Disposition ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsBearer ;
                                   owl:allValuesFrom SOMA:CutObject
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsTrigger ;
                                   owl:allValuesFrom SOMA:Cutter
                                 ] ;
                 rdfs:comment "The disposition of an object (the barrier) which makes it able to be cut down (into pieces) usually by some other object with role as a Cutter. Such as a cucumber has cuttability disposition which can be cut by a knife." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cutter
SOMA:Cutter rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Tool ;
            rdfs:comment "A role to classify an object used to cut other objects. Usually should poses sharpness as a quality. Execptions are not considered in this context. Such as a wind, water, or other natural agents cutting(eroding) the rocks." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cutting
SOMA:Cutting rdf:type owl:Class ;
             rdfs:subClassOf SOMA:ModifyingPhysicalObject ;
             rdfs:comment "The goal of this task is to separate one or more pieces from some target object by means of cutting into its constituent material. Unlike a disassembly, a cut is usually not easily reversible." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CylinderShape
SOMA:CylinderShape rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:ShapeRegion ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty SOMA:hasRadius ;
                                     owl:someValuesFrom xsd:float
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty SOMA:hasLength ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:float
                                   ] ;
                   rdfs:comment "A solid geometrical figure with straight parallel sides and a circular or oval cross section." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Cylinder shape" .


###  http://www.ease-crc.org/ont/SOMA.owl#Database
SOMA:Database rdf:type owl:Class ;
              rdfs:subClassOf SOMA:SoftwareRole ;
              rdfs:comment """A Database is a Software that organizes a collection of data stored and and allows for access, usually via some query engine.

Source: https://en.wikipedia.org/wiki/Database"""@en ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Deciding
SOMA:Deciding rdf:type owl:Class ;
              rdfs:subClassOf SOMA:DerivingInformation ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                               "A mental task in where an agent makes some decision, that is, selecting some Information object; usually on the basis of others."@en ;
              rdfs:label "Deciding"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#DeclarativeClause
SOMA:DeclarativeClause rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:ClausalObject ;
                       rdfs:comment "A clause which makes an assertion or declaration." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                       rdfs:label "Declarative clause" .


###  http://www.ease-crc.org/ont/SOMA.owl#DeductiveReasoning
SOMA:DeductiveReasoning rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:Reasoning ;
                        rdfs:comment """A task in which the Agent, using general logical principles that it considers logically valid, applied to premises that it considers logically true, arrives at conclusions that it considers logically certain.

Deduction is often explained as starting from the \"general\" (some property X is known about all members of a set S), applying it to the \"specific\" (some Entity Y is known to belong to set S), to arrive at a specialization of the general property (X applies to Y).""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                        rdfs:label "Deductive reasoning"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Deformation
SOMA:Deformation rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Alteration ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:isProcessTypeOf ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass SOMA:ShapedObject
                                 ] ;
                 owl:disjointWith SOMA:FluidFlow ;
                 rdfs:comment """A process by which a physical Entity changes its shape under the influence of some factors outside of the Entity.

Note, changes of shape may be self-caused; for example, a gas will naturally disperse. This however would be a different type of process (Dispersion).

A soft slab of clay deforming under its own weight on Earth would still count as deformation: it is the gravity of the Earth (so, a factor outside the slab of clay) which makes the slab change shape.""" ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Delivering
SOMA:Delivering rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Actuating ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:isTaskAffordedBy ;
                                  owl:someValuesFrom SOMA:Shifting
                                ] ;
                owl:disjointWith SOMA:Fetching ,
                                 SOMA:Lifting ,
                                 SOMA:Opening ,
                                 SOMA:Pulling ,
                                 SOMA:Pushing ,
                                 SOMA:Squeezing ;
                rdfs:comment "A task in which an Agent brings an item that the Agent already carries to a specified target." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DependentPlace
SOMA:DependentPlace rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Feature ;
                    rdfs:comment "A feature that is not part of its host, like a hole in a piece of cheese." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Dependent place"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Deposit
SOMA:Deposit rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Instrument ;
             rdfs:comment "A role classifying an object ontop which others are put to e.g. store them, or to place them in a meaningful way for future activities." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DepositedObject
SOMA:DepositedObject rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Patient ;
                     rdfs:comment "An object placed ontop of another one." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Deposited object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Deposition
SOMA:Deposition rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Disposition ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:affordsBearer ;
                                  owl:allValuesFrom SOMA:Deposit
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:affordsTrigger ;
                                  owl:allValuesFrom SOMA:DepositedObject
                                ] ;
                rdfs:comment "The disposition to support objects." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DerivingInformation
SOMA:DerivingInformation rdf:type owl:Class ;
                         rdfs:subClassOf SOMA:InformationAcquisition ,
                                         [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty SOMA:isTaskOfInputRole ;
                                                                  owl:someValuesFrom SOMA:Premise
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty SOMA:isTaskOfOutputRole ;
                                                                  owl:someValuesFrom SOMA:Conclusion
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                         rdfs:label "Deriving information" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedComponent
SOMA:DesignedComponent rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:FunctionalPart ,
                                       <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:hasDisposition ;
                                         owl:someValuesFrom SOMA:Connectivity
                                       ] ;
                       rdfs:comment "An object designed to be part or element of a larger whole." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Designed component" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedContainer
SOMA:DesignedContainer rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:hasDisposition ;
                                         owl:someValuesFrom SOMA:Containment
                                       ] ;
                       rdfs:comment "An item designed to be able to hold some other items, preventing their free movement and/or protecting them from outside influence. Containers may be used for storage, or to obtain control over items that are otherwise hard to manipulate directly (e.g. liquids)." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Designed container" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedFurniture
SOMA:DesignedFurniture rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ;
                       rdfs:comment "An object used to make a room or building suitable for living or working." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Designed furniture" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedTool
SOMA:DesignedTool rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ;
                  rdfs:comment "An item designed to enable some action, in which it will play an instrumental role." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                  rdfs:label "Designed tool" .


###  http://www.ease-crc.org/ont/SOMA.owl#Destination
SOMA:Destination rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Location ;
                 rdfs:comment "A role classifying the location where an event or object is directed towards." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DestroyedObject
SOMA:DestroyedObject rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Patient ;
                     rdfs:comment "An object that is detroyed." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                     rdfs:label "Destroyed object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Destruction
SOMA:Destruction rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:ProcessType ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:isProcessTypeOf ;
                                   owl:someValuesFrom SOMA:DestroyedObject
                                 ] ;
                 rdfs:comment """A process by which a physical Entity is destroyed.

Note, most of the physical Entities we are concerned with are actually arrangements of many smaller physical Entities, so another way to look at this is that a 'Physical destruction' is a process by which an arrangement of physical Entities, which was previously itself considered a physical Entity, is changed to such an extent that it is no longer recognized as continuing to exist.""" ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                 rdfs:label "Destruction" .


###  http://www.ease-crc.org/ont/SOMA.owl#DetectedObject
SOMA:DetectedObject rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Patient ;
                    rdfs:comment "An object that is detected." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Detected object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#DeviceState
SOMA:DeviceState rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Intrinsic ;
                 rdfs:comment "A quality belonging to a device which indicates its overall functional state." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DeviceStateRange
SOMA:DeviceStateRange rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Class ;
                                            owl:unionOf ( SOMA:DeviceTurnedOff
                                                          SOMA:DeviceTurnedOn
                                                        )
                                          ] ;
                      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region> ;
                      rdfs:comment "This class defines the values that a device state can take." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                      rdfs:label "Device state range"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#DeviceTurnedOff
SOMA:DeviceTurnedOff rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:DeviceStateRange ;
                     rdfs:comment "A value indicating a device is not in operation." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Device turned off"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#DeviceTurnedOn
SOMA:DeviceTurnedOn rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:DeviceStateRange ;
                    rdfs:comment "A value indicating a device is in operation." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Device turned on"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#DexterityDiagnosis
SOMA:DexterityDiagnosis rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:BehavioralDiagnosis ;
                        rdfs:comment "A description of the dexterity of a system, possibly in comparison to another system." ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> ;
                        rdfs:label "Dexterity diagnosis" .


###  http://www.ease-crc.org/ont/SOMA.owl#Dicing
SOMA:Dicing rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Cutting ;
            rdfs:comment "A particular kind of cutting where the goal is to produce small pieces out of some object or material. Unlike slices, the pieces to be obtained do not have one or two dimensions being more prominent than others. \"Dice\", the pieces dicing results in, are approximately cubic." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Digital_File
SOMA:Digital_File rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationRealization> ,
                                  _:genid277 ,
                                  _:genid279 ;
                  rdfs:comment "Any file that exists as a digital resource (but not its content), e.g., a text file actually laying on some hard drive, but not the contained text."@en ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                  rdfs:label "Computer File"@en ,
                             "Digital File"@en .

_:genid277 rdf:type owl:Restriction ;
            owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes> ;
            owl:someValuesFrom SOMA:Structured_Text .

_:genid279 rdf:type owl:Restriction ;
            owl:onProperty SOMA:hasNameString ;
            owl:someValuesFrom xsd:string .

[ rdf:type owl:Axiom ;
   owl:annotatedSource SOMA:Digital_File ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget _:genid277 ;
   rdfs:comment "The file content."@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource SOMA:Digital_File ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget _:genid279 ;
   rdfs:comment "The path of the file on a computer system."@en
 ] .


###  http://www.ease-crc.org/ont/SOMA.owl#DirectedMotion
SOMA:DirectedMotion rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Motion ;
                    owl:disjointWith SOMA:UndirectedMotion ;
                    rdfs:comment "A Motion that is considered to be toward a location or along a path. It is not important that the final location or path be the intention of some Agent, but it is considered that the important feature of this Motion is that it has a path and/or destination." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                    rdfs:label "Directed motion" .


###  http://www.ease-crc.org/ont/SOMA.owl#Dirty
SOMA:Dirty rdf:type owl:Class ;
           rdfs:subClassOf SOMA:CleanlinessRegion ;
           rdfs:comment "A cleanliness region with values considered as dirty." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Discourse
SOMA:Discourse rdf:type owl:Class ;
               rdfs:subClassOf SOMA:CommunicationTask ;
               rdfs:comment "A mental task, in which two or more agents discuss some topic via multiple Illocutionary acts, which are part of this task."@en ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
               rdfs:label "Discourse"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Disposition
SOMA:Disposition rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Extrinsic ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy> ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass SOMA:Affordance
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy> ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass [ owl:intersectionOf ( SOMA:Affordance
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty SOMA:definesBearer ;
                                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role>
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty SOMA:definesEventType ;
                                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#EventType>
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty SOMA:definesTrigger ;
                                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role>
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ]
                                 ] ;
                 rdfs:comment "The tendency of an object (the bearer) to make certain events happen with others (the triggers)." ,
                              "extrinsic" ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Distancing
SOMA:Distancing rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Navigating ;
                rdfs:comment "A task in which an Agent increases its distance from some location." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Dreaming
SOMA:Dreaming rdf:type owl:Class ;
              rdfs:subClassOf SOMA:DerivingInformation ;
              rdfs:comment "Any form of re-processing episodic memories for long-term memory by natural or aritifical agents." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Driving
SOMA:Driving rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Locomotion ;
             owl:disjointWith SOMA:Flying ,
                              SOMA:Swimming ,
                              SOMA:Walking ;
             rdfs:comment "A process type classifying a motion of a body that exists because this body is attached to and controls some other moving body, usually a vehicle." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Dropping
SOMA:Dropping rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Actuating ;
              owl:disjointWith SOMA:Placing ;
              rdfs:comment """The dropped object falls mainly under the influence of gravity. However, an agent may also drop something during navigation. The difference to 'Throwing' is that there is no 'Limb motion' which is a constitiuent of the action.

'Dropping' is intentional. Dropping by accident may not has a phase to release the grasp. It could be that the grasp was not strong enough and the objects \"slips\" away.""" ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ESTSchemaTheory
SOMA:ESTSchemaTheory rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:ImageSchemaTheory ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                       owl:someValuesFrom SOMA:ExistingObjectRole
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                       owl:cardinality "1"^^xsd:nonNegativeInteger
                                     ] ;
                     rdfs:comment """A schematic theory that describes the existence of an entity.

Developmental psychology posits that \"object permanence\" (the assumption that physical objects keep existing even when the agent doesn't perceive them, which consequently informs reasoning about where an object should be, even when perception of it is lost) is a cognitive ability that is not, at least in the very young human child, immediately manifest. Rather, it seems learned via interaction, and usually is among an infant's cognitive repertoire by age 2.

In SOMA, we represent this ability of a cognitive agent as an ability to generate and maintain ESTSchemaTheories. Each instance of such a theory refers to one particular physical object, the one that the instance of the ESTSchemaTheory asserts to exist.

Because each instance of an ESTSchemaTheory refers to a single object, ESTSchemaTheories are not associated to any relation between OWL individuals.""" ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                     rdfs:label "EST schema theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Effort
SOMA:Effort rdf:type owl:Class ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter> ;
            rdfs:comment "A parameter describing the amount of force to be exerted by some actuator." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#EnclosedObject
SOMA:EnclosedObject rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:IncludedObject ;
                    rdfs:comment "An object included within the spatial boundaries of another object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Enclosed object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Enclosing
SOMA:Enclosing rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Containment ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:affordsTrigger ;
                                 owl:allValuesFrom SOMA:EnclosedObject
                               ] ;
               rdfs:comment "The disposition of an object (the container) to contain other objects by enclosing them to prevent their free movement." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#EndEffectorPositioning
SOMA:EndEffectorPositioning rdf:type owl:Class ;
                            rdfs:subClassOf SOMA:Manipulating ;
                            rdfs:comment "A task in which an Agent places its end effectors at particular poses." ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                            rdfs:label "End effector positioning"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Episode
SOMA:Episode rdf:type owl:Class ;
             rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
             rdfs:comment "" ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#EventAdjacentRole
SOMA:EventAdjacentRole rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role> ;
                       rdfs:comment """A role classifying a participant in an event.

In linguistics, this is also known as a thematic role.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Event adjacent role"@en ,
                                  "Thematic role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExcludedObject
SOMA:ExcludedObject rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Patient ;
                    rdfs:comment "An object that is not contained in something. This is meant very general and includes, e.g., elements excluded from a set, or things that are spatially excluded from the boundaries of some object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Excluded object" .


###  http://www.ease-crc.org/ont/SOMA.owl#ExecutableFile
SOMA:ExecutableFile rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( SOMA:Digital_File
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes> ;
                                                                 owl:someValuesFrom SOMA:Executable_Code
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:comment """An Executable File, sometimes simply referred to as an executable or binary, causes a computer \"to perform indicated tasks according to encoded instructions\", as opposed to a data file that must be interpreted (parsed) by a program to be meaningful.

The exact interpretation depends upon the use. \"Instructions\" is traditionally taken to mean machine code instructions for a physical CPU. In some contexts, a file containing scripting instructions (such as bytecode) may also be considered executable."""@en ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                    rdfs:label "Executable File"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExecutableFormat
SOMA:ExecutableFormat rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:File_format ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty SOMA:givesMeaningTo ;
                                        owl:allValuesFrom SOMA:Executable_Code
                                      ] ;
                      rdfs:comment "An Executable Format is a File Format that allows computers to directly execute the content. Examples are MZ (DOS) or COFF."@en ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                      rdfs:label "Executable Format"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExecutableSchematicTheory
SOMA:ExecutableSchematicTheory rdf:type owl:Class ;
                               rdfs:subClassOf SOMA:SchematicTheory ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                                 owl:someValuesFrom SOMA:PerformerRole
                                               ] ;
                               rdfs:comment """Also known as \"executing schemas\" or \"x-schemas\", these were defined by Bergen and Chang in their work \"Embodied Construction Grammar in Simulation-Based Language Understanding\" as:

\"Executing schemas, or x-schemas, are dynamic representations motivated in part by motor and perceptual systems (Bailey 1997; Narayanan 1997), on the assumption that the same underlying representations used for executing and perceiving an action are brought to bear in understanding language about that action. The x-schema formalism is an extension of Petri nets (Murata 1989) that can model sequential, concurrent, and
asynchronous events\"

SOMA does not restrict the formalism of ExecutableSchematicTheories; i.e. they don't have to be Petri Nets.

They maintain their role however as representations able to drive a simulation, at some level of abstraction, of an embodied action. This level of abstraction may be still fairly underspecified as in the case of the original x-schemas and as such not a plan that an agent could run in an actual physical environment without further information.""" ;
                               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                               rdfs:label "Executable schematic theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExecutableSoftware
SOMA:ExecutableSoftware rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( SOMA:Software
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasMember> ;
                                                                     owl:someValuesFrom SOMA:ExecutableFile
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:comment "Executable Software is Software that can directly be executed, in comparison to a Software Library, which might only contain functionality via interfaces, but offers no execution entry point."@en ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                        rdfs:label "Executable Software"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Executable_Code
SOMA:Executable_Code rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( SOMA:Computer_Program
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty SOMA:isGivenMeaningBy ;
                                                                  owl:someValuesFrom SOMA:ExecutableFormat
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf SOMA:Computer_Program ;
                     rdfs:comment "Executable Code is Code that when compiled / interpreted, has some clear entrance point and can be executed. Note the difference to an Executable File, which is the file that contains such (compiled) code."@en ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                     rdfs:label "Executable Code"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExecutionStateRegion
SOMA:ExecutionStateRegion rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Class ;
                                                owl:oneOf ( SOMA:ExecutionState_Active
                                                            SOMA:ExecutionState_Cancelled
                                                            SOMA:ExecutionState_Failed
                                                            SOMA:ExecutionState_Paused
                                                            SOMA:ExecutionState_Pending
                                                            SOMA:ExecutionState_Succeeded
                                                          )
                                              ] ;
                          rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region> ;
                          rdfs:comment "A region containing labels that describe different states in the evolution/completion of a task execution." ;
                          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                          rdfs:label "Execution state region" .


###  http://www.ease-crc.org/ont/SOMA.owl#ExistingObjectRole
SOMA:ExistingObjectRole rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:RelationAdjacentRole ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                          owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                        ] ;
                        rdfs:comment """A role that requires of its filler simply to exist, unlike other roles that may demand e.g. agentive or instrumental participation in some executable schema or plan (AgentRole and Instrument respectively).

The intention behind such a simple role is to have a way to represent, in a schematic formalism used to describe some scene, that an object is present. In particular, the schema used to convey this information is the ESTSchemaTheory, which has ExistingObjectRole as its sole defined role.""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                        rdfs:label "Existing object role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExperiencerRole
SOMA:ExperiencerRole rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:PerformerRole ;
                     rdfs:comment "A role used in frame semantics to classify agents performing perception actions, or being the subjects affected by some biological process." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Experiencer role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ExtractedObject
SOMA:ExtractedObject rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Patient ;
                     rdfs:comment "An object that is removed from a container or system." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Extracted object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Extrinsic
SOMA:Extrinsic rdf:type owl:Class ;
               rdfs:subClassOf SOMA:PhysicalQuality ;
               owl:disjointWith SOMA:Intrinsic ;
               rdfs:comment "A physical quality that depends on relationships to other objects, such as the color of an object which depends on light conditions in the environment." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#FactualBinding
SOMA:FactualBinding rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Binding ;
                    rdfs:comment """FactualBindings are used in a workflow to assert that:

-- task- or workflow-defined roles share fillers (RoleRoleBindings). Example, the instrument of a cutting task may be the actee of a grasping task;
-- task- or workflow-defined roles are filled by a particular entity (RoleFillerBindings). This is typically the case when roles, and especially parameters, can be assigned to constants. Example, the waiting time to cook soft eggs is 3 minutes.""" ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-WF.owl> ;
                    rdfs:label "Factial binding"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#FailedAttempt
SOMA:FailedAttempt rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Unsuccessfulness ;
                   rdfs:comment "A description of a failed attempt to achieve some goal." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> ;
                   rdfs:label "Failed attempt"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#FaultySoftware
SOMA:FaultySoftware rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:SoftwareDiagnosis ;
                    rdfs:comment "A description of a situation where some software has a bug." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                    rdfs:label "Faulty software"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Feature
SOMA:Feature rdf:type owl:Class ;
             rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart> ;
                               owl:allValuesFrom SOMA:Feature
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty SOMA:isFeatureOf ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                             ] ;
             owl:disjointWith <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ,
                              <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject> ,
                              <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject> ;
             rdfs:comment "Features are 'parasitic' entities that only exist insofar their host exists. Typical examples are holes, bumps, boundaries, or spots of color." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Fetching
SOMA:Fetching rdf:type owl:Class ;
              rdfs:subClassOf SOMA:PhysicalAcquiring ;
              owl:disjointWith SOMA:Lifting ,
                               SOMA:Opening ,
                               SOMA:Pulling ,
                               SOMA:Pushing ,
                               SOMA:Squeezing ;
              rdfs:comment "A task in which an Agent retrieves an object from a particular location, which puts the object under the Agent's control, who can now e.g. transport the object somewhere else; this task may include repositioning the Agent to better reach the object. Note that while in normal linguistic use fetch can mean transport, we use it here to refer only to (a part of) the first stage of transport." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#FileConfiguration
SOMA:FileConfiguration rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Configuration>
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasMember> ;
                                                                    owl:allValuesFrom SOMA:Digital_File
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:comment "A configuration whose members are all files. Used to model, e.g., concrete collections of zip- and jar-files (and so on)."@en ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                       rdfs:label "File Configuration"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#File_format
SOMA:File_format rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Computer_Language ;
                 rdfs:comment """A File Format is a standard way that information is encoded for storage in a computer file. It specifies how bits are used to encode information in a digital storage medium.

From Wikipedia: https://en.wikipedia.org/wiki/File_format"""@en ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                 rdfs:label "File Format"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Finger
SOMA:Finger rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Limb ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf> ;
                              owl:someValuesFrom SOMA:Hand
                            ] ;
            rdfs:comment "A limb used for grasping objects." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#FixedJoint
SOMA:FixedJoint rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Joint ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointState ;
                                  owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity>
                                ] ;
                owl:disjointWith SOMA:MovableJoint ;
                rdfs:comment "A joint that cannot move, designed to fixiate links." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Fixed joint" .


###  http://www.ease-crc.org/ont/SOMA.owl#Flipping
SOMA:Flipping rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Actuating ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:isTaskAffordedBy ;
                                owl:someValuesFrom SOMA:Shifting
                              ] ;
              rdfs:comment "The task in which the agent turns an object over by using a tool or by manipulating" ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#FloatingJoint
SOMA:FloatingJoint rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:MovableJoint ;
                   rdfs:comment "A joint that allows motion for all 6 degrees of freedom." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Floating joint" .


###  http://www.ease-crc.org/ont/SOMA.owl#Fluid
SOMA:Fluid rdf:type owl:Class ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Substance> ;
           rdfs:comment "A substance with a consistency such that it can flow or diffuse." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ,
                            <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#FluidFlow
SOMA:FluidFlow rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Motion ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:isProcessTypeOf ;
                                 owl:someValuesFrom SOMA:MovedObject
                               ] ;
               rdfs:comment """A process by which a fluid moves or is moved from a location to another, but such that it maintains its constitution. A fluid is an Entity made of many smaller Entities, loosely bound to each other.

An issue to highlight here is the maintenance of constitution. Fluids-- gases in particular-- are prone to disperse. Such a process is not flow however, because the loose bounds between the constituents become even looser, to the point of the original Entity becoming entirely discombobulated. """ ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
               rdfs:label "Fluid flow" .


###  http://www.ease-crc.org/ont/SOMA.owl#Flying
SOMA:Flying rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Locomotion ;
            owl:disjointWith SOMA:Swimming ,
                             SOMA:Walking ;
            rdfs:comment "A process type classifying a motion of a body that, through its own power, keeps itself aloft." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Focusing
SOMA:Focusing rdf:type owl:Class ;
              rdfs:subClassOf SOMA:AttentionShift ;
              rdfs:comment "The mental task to center the attention to some subject." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
              rdfs:label "Focusing" .


###  http://www.ease-crc.org/ont/SOMA.owl#Foolishness
SOMA:Foolishness rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Amateurish ;
                 rdfs:comment "A description of foolish behavior." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ForceAttribute
SOMA:ForceAttribute rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty SOMA:hasForceValue ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange SOMA:array_double
                                    ] ;
                    rdfs:comment "The value of a force dynamical characteristic. An example is the force exerted on another object when pushing it." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Force attribute" .


###  http://www.ease-crc.org/ont/SOMA.owl#ForceInteraction
SOMA:ForceInteraction rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Class ;
                                            owl:unionOf ( SOMA:AlterativeInteraction
                                                          SOMA:PreservativeInteraction
                                                        )
                                          ] ;
                      rdfs:subClassOf SOMA:ProcessType ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty SOMA:isProcessTypeOf ;
                                        owl:someValuesFrom SOMA:Agonist
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty SOMA:isProcessTypeOf ;
                                        owl:someValuesFrom SOMA:Antagonist
                                      ] ;
                      rdfs:comment "Classifies events in which two entities interact with each other with a reference to force. One of the entities, the agonist, has a tendency to either set the other entity (the antagonist) into motion, or to keep it still under the influence of some other force. The tendency only manifests in case the agonist is the \"stronger entity\"." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                      rdfs:label "Force interaction" ;
                      rdfs:seeAlso "Talmy, Leonard (1981) Force Dynamics. Language and Mental Imagery. May 1981, University of California, Berkeley." .


###  http://www.ease-crc.org/ont/SOMA.owl#ForgettingIncorrectInformation
SOMA:ForgettingIncorrectInformation rdf:type owl:Class ;
                                    rdfs:subClassOf SOMA:InformationDismissal ;
                                    rdfs:comment "A mental task in which the executing agent aims to correct its present information by deleting an incorrect information."@en ;
                                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                    rdfs:label "Forgetting incorrect information"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ForgettingIrrelevantInformation
SOMA:ForgettingIrrelevantInformation rdf:type owl:Class ;
                                     rdfs:subClassOf SOMA:InformationDismissal ;
                                     rdfs:comment "A mental task in which the executing agent aims to clean up its present information by deleting an irrelevant information."@en ;
                                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                     rdfs:label "Forgetting irrelevant information"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#FormalLanguage
SOMA:FormalLanguage rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Language ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty SOMA:givesMeaningTo ;
                                      owl:allValuesFrom SOMA:Structured_Text
                                    ] ;
                    rdfs:comment """A Formal Language consists of words whose letters are taken from an alphabet and are well-formed according to a specific set of rules.

From Wikipedia: https://en.wikipedia.org/wiki/Formal_language"""@en ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                    rdfs:label "Formal Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#FrictionAttribute
SOMA:FrictionAttribute rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:hasFrictionValue ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:double
                                       ] ;
                       rdfs:comment "The resistance that one surface or object encounters when moving over another." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Friction attribute" .


###  http://www.ease-crc.org/ont/SOMA.owl#FunctionalControl
SOMA:FunctionalControl rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:StateType ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:isStateTypeOf ;
                                         owl:someValuesFrom SOMA:Item
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:isStateTypeOf ;
                                         owl:someValuesFrom SOMA:Restrictor
                                       ] ;
                       rdfs:comment """Classifies States in which an object restricts the movement of another, at least partially. Usually neither object is construed to be an agent.

Note that this State focuses on how the interaction of, usually non-agentive, objects restricts their motion. This is in contrast to Blockage/Accessibility states where the placement of some objects influences the access to some of them by a third, usually agentive party.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                       rdfs:label "Functional state"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#FunctionalDesign
SOMA:FunctionalDesign rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Design> ;
                      rdfs:comment "The design of an object from functionality point of view. A functional design is useful to develop complex modular objects with components that have a specific purpose, and can function with minimum side effect on other components of that object. " ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                      rdfs:label "Functional Design" .


###  http://www.ease-crc.org/ont/SOMA.owl#FunctionalDiagnosis
SOMA:FunctionalDiagnosis rdf:type owl:Class ;
                         rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Diagnosis> ;
                         rdfs:comment "An internal diagnosis of a system." ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> ;
                         rdfs:label "Functional diagnosis" .


###  http://www.ease-crc.org/ont/SOMA.owl#FunctionalPart
SOMA:FunctionalPart rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalBody> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isComponentOf> ;
                                      owl:allValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                        <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact>
                                                                      )
                                                        ]
                                    ] ;
                    rdfs:comment "Parts of an agent or an artifact are considered as functional parts." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Functional part" .


###  http://www.ease-crc.org/ont/SOMA.owl#FunctionalSpatialSchemaTheory
SOMA:FunctionalSpatialSchemaTheory rdf:type owl:Class ;
                                   rdfs:subClassOf SOMA:ImageSchemaTheory ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                                     owl:someValuesFrom SOMA:LocatumRole
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                                     owl:someValuesFrom SOMA:RelatumRole
                                                   ] ;
                                   rdfs:comment """The superclass for theories describing functional spatial relations.

Adopted from GUM-4-space (Bateman et al. 2010).""" ;
                                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                                   rdfs:label "Functional spatial schema theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#GetTaskParameter
SOMA:GetTaskParameter rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:Planning ;
                      rdfs:comment "A task in which an Agent computes some parameter relevant for another task." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                      rdfs:label "Get task parameter"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#GoalRole
SOMA:GoalRole rdf:type owl:Class ;
              rdfs:subClassOf SOMA:EventAdjacentRole ;
              rdfs:comment "A role classifying objects that constitute the goal of an action." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
              rdfs:label "Goal role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#GraphDatabase
SOMA:GraphDatabase rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Database ;
                   rdfs:comment "A Graph Database is a Database that uses graph structures for semantic queries with nodes, edges, and properties to represent and store data."@en ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                   rdfs:label "Graph database"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#GraphQueryLanguage
SOMA:GraphQueryLanguage rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:QueryLanguage ;
                        rdfs:comment "A Query Language that is designed for communication with some Graph Database."@en ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                        rdfs:label "Graph query language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#GraspTransfer
SOMA:GraspTransfer rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Grasping ;
                   rdfs:comment "A task in which an Agent switches which of its end effectors holds an object." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                   rdfs:label "Grasp transfer"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Graspability
SOMA:Graspability rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Disposition ;
                  rdfs:comment "The disposition of an object (e.g. the handle) to afford grasping the object." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Grasping
SOMA:Grasping rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Manipulating ;
              owl:disjointWith SOMA:Releasing ;
              rdfs:comment "A task in which an Agent uses its end effectors to grasp an object, thus gaining kinematic control over it." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#GraspingMotion
SOMA:GraspingMotion rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:unionOf ( SOMA:IntermediateGrasp
                                                        SOMA:PowerGrasp
                                                        SOMA:PrecisionGrasp
                                                      )
                                        ] ;
                    rdfs:subClassOf SOMA:PrehensileMotion ;
                    owl:disjointWith SOMA:ReleasingMotion ;
                    rdfs:comment "A process type classifying a motion by which some end effector acquires kinematic control over some other object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                    rdfs:label "Grasping motion" .


###  http://www.ease-crc.org/ont/SOMA.owl#GreenColor
SOMA:GreenColor rdf:type owl:Class ;
                rdfs:subClassOf SOMA:ColorRegion ;
                rdfs:comment "A color region with dominant green color." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Green color" .


###  http://www.ease-crc.org/ont/SOMA.owl#Gripper
SOMA:Gripper rdf:type owl:Class ;
             rdfs:subClassOf SOMA:PrehensileEffector ;
             rdfs:comment "A mechanical device that grasps and holds things." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Hand
SOMA:Hand rdf:type owl:Class ;
          rdfs:subClassOf SOMA:PrehensileEffector ;
          rdfs:comment "A prehensile effector including palm, fingers, and thumb." ;
          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#HardwareDiagnosis
SOMA:HardwareDiagnosis rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:TechnicalDiagnosis ;
                       rdfs:comment "A diagnosis of the hardware of a system." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> ;
                       rdfs:label "Hardware diagnosis" .


###  http://www.ease-crc.org/ont/SOMA.owl#HasQualityRegion
SOMA:HasQualityRegion rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Relation> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality>
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region>
                                      ] ;
                      rdfs:comment "The relation between an individual quality and a region." ,
                                   "todo(DB): added for NEEMs (quale change), but not sure yet about it..." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                      rdfs:label "Has quality region"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Head
SOMA:Head rdf:type owl:Class ;
          rdfs:subClassOf SOMA:FunctionalPart ;
          rdfs:comment "A functional part of the body responsible for carrying high bandwidth sensors, i.e., camera." ;
          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#HeadMovement
SOMA:HeadMovement rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:BodyMovement ;
                  rdfs:comment "The Agent moves a part of their body carrying high-bandwidth sensors such as cameras. The movement of other body parts is not significant." ,
                               "todo: (Mihai:) This seems too specific, given the \"Directed\"/\"Undirected motion\" categories." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#HeadTurning
SOMA:HeadTurning rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:HeadMovement ;
                 rdfs:comment "A process type classifying a motion of an Agent's head such that the direction this head faces changes relative to the facing direction of the Agent's body as a whole." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#HingeJoint
SOMA:HingeJoint rdf:type owl:Class ;
                rdfs:subClassOf SOMA:MovableJoint ;
                rdfs:comment "A joint that rotates along an axis." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Hinge joint" .


###  http://www.ease-crc.org/ont/SOMA.owl#Holding
SOMA:Holding rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Manipulating ;
             rdfs:comment "A task by which an Agent keeps an object over which it has kinematic control, typically via grasping, at some specified pose." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#HostRole
SOMA:HostRole rdf:type owl:Class ;
              rdfs:subClassOf SOMA:InterfaceComponentRole ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn> ;
                                owl:someValuesFrom SOMA:PluginSpecification
                              ] ;
              rdfs:comment """In the Plug-in-Host pattern, a Host application provides services which the Plug-in can use, including a way for Plug-ins to register themselves with the Host application and a protocol for the exchange of data withPplug-ins. Plug-ins depend on the services provided by the host application and do not usually work by themselves. Conversely, the host application operates independently of the plug-ins, making it possible for end-users to add and update plug-ins dynamically without needing to make changes to the host application.

Source: https://en.wikipedia.org/wiki/Plug-in_(computing)"""@en ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
              rdfs:label "Host role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Human-readable_Programming_Language
SOMA:Human-readable_Programming_Language rdf:type owl:Class ;
                                         rdfs:subClassOf SOMA:Programming_Language ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty SOMA:givesMeaningTo ;
                                                           owl:allValuesFrom SOMA:Source_Code
                                                         ] ;
                                         rdfs:comment "A Programming language like Java, Python etc. but not binary machine code."@en ;
                                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                                         rdfs:label "Human-readable Programming Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#HumanActivityRecording
SOMA:HumanActivityRecording rdf:type owl:Class ;
                            rdfs:subClassOf SOMA:RecordedEpisode ;
                            rdfs:comment "An episode in which one or more human beings perform an activity and are recorded doing so." ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#IllocutionaryTask
SOMA:IllocutionaryTask rdf:type owl:Class ;
                       rdfs:subClassOf [ owl:intersectionOf ( SOMA:CommunicationTask
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                                                owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                                        owl:unionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                                                                      <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject>
                                                                                                                    )
                                                                                                      ]
                                                                                  ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                       rdfs:comment """A task which is executed by a Locution action: A Locution is what was said and meant, Illocution is what was done.

When somebody says \"Is there any salt?\" at the dinner table, the illocutionary act is a request: \"please give me some salt\" even though the locutionary act (the literal sentence) was to ask a question about the presence of salt.

Source: https://en.wikipedia.org/wiki/Illocutionary_act"""@en ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                       rdfs:label "Illocutionary act"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ImageSchemaTheory
SOMA:ImageSchemaTheory rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:SchematicTheory ;
                       rdfs:comment """A theory that describes an image-schematic relationship between some entities.

Image schemas are considered as fundamental, pre-conceptual, building blocks of cognition. They were introduced to characterize how human beings are believed to organize and make sense of experience.

For SOMA, whereas the focus of executable schematic theories is to describe how an agent might act, image schematic theories focus on descriptions of how objects behave in the absence of, or after, an active intervention from an agent.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                       rdfs:label "Image schema theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Imagining
SOMA:Imagining rdf:type owl:Class ;
               rdfs:subClassOf SOMA:InformationAcquisition ;
               rdfs:comment "A Mental task in which the Agent constructs a mental representation of a possible world. An Agent performing an Imagining activity does not aim to construct a representation that aspires to be faithful to some past, present, or future state of affairs of the actual world it is embodied in." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Impediment
SOMA:Impediment rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Blockage ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:affordsBearer ;
                                  owl:allValuesFrom SOMA:Obstacle
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:affordsTrigger ;
                                  owl:allValuesFrom SOMA:RestrictedObject
                                ] ;
                rdfs:comment "The disposition of an object (the obstacle) to prohibit certain ways of entering or leaving a space or group. An example is a doorstopper constraining a door, prohibiting it to enter the area behind it." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ImperativeClause
SOMA:ImperativeClause rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:ClausalObject ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses> ;
                                        owl:someValuesFrom SOMA:StateTransition
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses> ;
                                        owl:allValuesFrom SOMA:StateTransition
                                      ] ;
                      rdfs:comment "A clause which commands some agent to perform a task or bring about a state of affairs." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                      rdfs:label "Imperative clause" .


###  http://www.ease-crc.org/ont/SOMA.owl#Inability
SOMA:Inability rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Unsuccessfulness ;
               rdfs:comment "A description of a situation with a goal that some system is unable to achieve." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#IncludedObject
SOMA:IncludedObject rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Patient ;
                    rdfs:comment "An object that is contained in something. This is meant very general and includes, e.g., elements contained in a set, or things that are spatially contained within the boundaries of some object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Included object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#IncompatibleSoftware
SOMA:IncompatibleSoftware rdf:type owl:Class ;
                          rdfs:subClassOf SOMA:SoftwareDiagnosis ;
                          rdfs:comment "A description of a situation where two software systems are incompatible with each other." ;
                          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                          rdfs:label "Incompatible software"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InductiveReasoning
SOMA:InductiveReasoning rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:Reasoning ;
                        rdfs:comment """A task in which the Agent endeavors to accumulate confidence in some general statement about the world, by gathering instances in which this general statement appears to apply. Note that perfect confidence can never be guaranteed by induction.

Induction is often described as a move from many \"specifics\" (swan A is white, swan B is white, swan C is white, ...) to the \"general\" (all swans are white).""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                        rdfs:label "Inductive resoning"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Infeasibility
SOMA:Infeasibility rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Unsuccessfulness ;
                   rdfs:comment "A description of a situation with a goal that is impossible to achieve in some situational context." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#InferenceRules
SOMA:InferenceRules rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Knowledge ;
                    rdfs:comment "The role of an object that is used to derive a conclusion from some premises." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                    rdfs:label "Inference rules" .


###  http://www.ease-crc.org/ont/SOMA.owl#InformationAcquisition
SOMA:InformationAcquisition rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( SOMA:MentalTask
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty SOMA:isTaskOfOutputRole ;
                                                                         owl:someValuesFrom SOMA:Knowledge
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:comment """A mental task in which the executing agent acquires some information that was not immediately available to it before.
A synonym might be \"Thinking\".

Examples include recalling knowledge or inferring some information from other information."""@en ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                            rdfs:label "Information acquisition"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InformationDismissal
SOMA:InformationDismissal rdf:type owl:Class ;
                          rdfs:subClassOf SOMA:MentalTask ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty SOMA:isTaskOfInputRole ;
                                            owl:someValuesFrom [ owl:intersectionOf ( SOMA:ExcludedObject
                                                                                      SOMA:Knowledge
                                                                                    ) ;
                                                                 rdf:type owl:Class
                                                               ]
                                          ] ;
                          rdfs:comment """A mental task in which the executing agent dismisses some information.

An example is forgetting some knowledge."""@en ;
                          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                          rdfs:label "Information dismissal"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InformationRetrieval
SOMA:InformationRetrieval rdf:type owl:Class ;
                          rdfs:subClassOf SOMA:InformationAcquisition ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty SOMA:isTaskOfOutputRole ;
                                            owl:someValuesFrom SOMA:Knowledge
                                          ] ;
                          rdfs:comment """A mental task in which an Agent recalls some knowledge that has been memorized previously.

Examples include a human remembering some information or a computer retrieving knowledge from a database.

The difference to Remembering is that for this Task, we are concerned with knowledge about a previous world state. Memory Retrieval is more general in the sense that it also includes the retrieval of learned facts and rules."""@en ;
                          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                          rdfs:label "Information retrieval"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InformationStorage
SOMA:InformationStorage rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:MentalTask ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty SOMA:isTaskOfInputRole ;
                                          owl:someValuesFrom [ owl:intersectionOf ( SOMA:Knowledge
                                                                                    SOMA:StoredObject
                                                                                  ) ;
                                                               rdf:type owl:Class
                                                             ]
                                        ] ;
                        rdfs:comment """A mental task in which the executing agent persists some information for later recall, if necessary.

An example is learning new knowledge."""@en ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                        rdfs:label "Information storage"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InsertedObject
SOMA:InsertedObject rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:EnclosedObject ;
                    rdfs:comment "An object inserted into another object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Inserted object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Insertion
SOMA:Insertion rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Enclosing ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:affordsTrigger ;
                                 owl:allValuesFrom SOMA:InsertedObject
                               ] ;
               rdfs:comment "The disposition of an object (the container) to contain other objects that can be inserted into the container through a portal." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Instructions
SOMA:Instructions rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Item ;
                  rdfs:comment "The role of a plan to follow during an execution task." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                  rdfs:label "Instructions" .


###  http://www.ease-crc.org/ont/SOMA.owl#Instrument
SOMA:Instrument rdf:type owl:Class ;
                rdfs:subClassOf SOMA:ResourceRole ;
                rdfs:comment "An object used to carry out the event." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-SAY.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#InterfaceComponentRole
SOMA:InterfaceComponentRole rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( SOMA:SoftwareRole
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn> ;
                                                                         owl:allValuesFrom SOMA:InterfaceSpecification
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf SOMA:SoftwareRole ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                            rdfs:label "Interface component role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InterfaceSpecification
SOMA:InterfaceSpecification rdf:type owl:Class ;
                            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Design> ;
                            rdfs:comment """The Specification of an Interface between software, computer hardware, peripheral devices, humans and combinations of these.

Source: https://en.wikipedia.org/wiki/Interface_(computing)"""@en ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                            rdfs:label "Interface Specification"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#IntermediateGrasp
SOMA:IntermediateGrasp rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:GraspingMotion ;
                       owl:disjointWith SOMA:PowerGrasp ,
                                        SOMA:PrecisionGrasp ;
                       rdfs:comment "A kind of grasp that acquires kinematic control over the gripped object, but without attempting to achieve either strong holding force nor precision of subsequent movement of the gripped object." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                       rdfs:label "Intermediate grasp" .


###  http://www.ease-crc.org/ont/SOMA.owl#Interpreting
SOMA:Interpreting rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:DerivingInformation ;
                  rdfs:comment "A task in which an Agent interpretes some information, e.g., makes sense of some incoming message or its visible surroundings."@en ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                  rdfs:label "Interpreting"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InterrogativeClause
SOMA:InterrogativeClause rdf:type owl:Class ;
                         rdfs:subClassOf SOMA:ClausalObject ;
                         rdfs:comment """A clause which makes a request, typically information, of some agent.

Note that in a semantic sense such clauses always request information, but in a pragmatic sense they can be used to convey commands or requests for action, such as e.g. \"can you close the door?\" The question is not just a request for information about ability, but a request to perform a task.""" ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                         rdfs:label "Interrogative clause" .


###  http://www.ease-crc.org/ont/SOMA.owl#Intrinsic
SOMA:Intrinsic rdf:type owl:Class ;
               rdfs:subClassOf SOMA:PhysicalQuality ;
               rdfs:comment "A physical quality that is independent of context." ,
                            "intrinsic" ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Introspecting
SOMA:Introspecting rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:InformationAcquisition ;
                   rdfs:comment "A mentalk task in which an Agent gathers and processes information about its own mental tasks via, e.g., Meta Reasoning."@en ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                   rdfs:label "Introspecting"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Item
SOMA:Item rdf:type owl:Class ;
          rdfs:subClassOf SOMA:Patient ;
          rdfs:comment "A role played by a non-agentive object operated on by an action." ;
          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                           <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Joint
SOMA:Joint rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( SOMA:FixedJoint
                                               SOMA:MovableJoint
                                             )
                               ] ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalBody> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasChildLink ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasParentLink ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasJointState ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass SOMA:JointState
                           ] ;
           rdfs:comment "An object that is used to articulate links in a kinematic structure." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#JointLimit
SOMA:JointLimit rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ;
                rdfs:comment "The physical limits of a joint." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Joint limit" .


###  http://www.ease-crc.org/ont/SOMA.owl#JointState
SOMA:JointState rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointPosition ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:double
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointVelocity ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:double
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointEffort ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:double
                                ] ;
                rdfs:comment "The state of a joint in terms of position, velocity of the joint and effort applied to it." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Joint state" .


###  http://www.ease-crc.org/ont/SOMA.owl#KineticFrictionAttribute
SOMA:KineticFrictionAttribute rdf:type owl:Class ;
                              rdfs:subClassOf SOMA:FrictionAttribute ;

SOMA:Limb rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( SOMA:Arm
                                              SOMA:Leg
                                            )
                              ] ;
          rdfs:subClassOf SOMA:PhysicalEffector ;
          rdfs:comment "An arm or leg of an embodied agent." ;
          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                           <http://www.ease-crc.org/ont/SOMA-PROC.owl> .



SOMA:LinkedObject rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:ConnectedObject ;
                  rdfs:comment "An object that is linked to some other object." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                  rdfs:label "Linked object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Localization
SOMA:Localization rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Extrinsic ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                    owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion>
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                    owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion>
                                  ] ;
                  rdfs:comment "The localization of an object. The region of this quality encodes values to localize the object in a dimensional space, e.g. Euclidean positions that localize the object in Euclidean space." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Location
SOMA:Location rdf:type owl:Class ;
              rdfs:subClassOf SOMA:SpatioTemporalRole ;
              rdfs:comment "A role classifying a location of interest, often specified as a spatial relation between several objects, themselves usually classified by spatial relation roles." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LocatumRole
SOMA:LocatumRole rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:SpatialRelationRole ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                   owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                 ] ;
                 rdfs:comment "Denotes the object with primary focal prominence in a spatial or spatio-temporal schema. Terminological variants that appear in the literature on cognitive linguistics include Figure (Talmy 1983) and Trajector (Langacker 1986)." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                 rdfs:label "Locatum role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Locomotion
SOMA:Locomotion rdf:type owl:Class ;
                rdfs:subClassOf SOMA:BodyMovement ,
                                SOMA:DirectedMotion ;
                owl:disjointWith SOMA:PrehensileMotion ;
                rdfs:comment """Conceptually related to Navigation, but distinguishable from it because of the profile, ie. the focus of the task.

Navigation is about reaching some goal.

Locomotion is concerned more with the actual motion.""" ,
                             "The Agent repositions their body in the environment." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LocutionaryAction
SOMA:LocutionaryAction rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( SOMA:CommunicationAction
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                    owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:comment """A Locutionary Act is the performance of an utterance (source: https://en.wikipedia.org/wiki/Locutionary_act).

We additionally require a Locutionary Act to be performed by an Agent, not an Actor - this is what sets it apart from a Communication Action."""@en ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                       rdfs:label "Locutionary action"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#LookingAt
SOMA:LookingAt rdf:type owl:Class ;
               rdfs:subClassOf SOMA:PhysicalTask ;
               rdfs:comment "better: Gazing" ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LookingFor
SOMA:LookingFor rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Perceiving ;
                rdfs:comment "A task by which an Agent uses its perception apparatus to check for the presence of an object in some specified area." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                rdfs:label "Looking for"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Lowering
SOMA:Lowering rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Actuating ;
              rdfs:comment "A task in which an Agent reduces the elevation at which they hold an item." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Manipulating
SOMA:Manipulating rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:PhysicalTask ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                    owl:allValuesFrom SOMA:PhysicalAction
                                  ] ;
                  rdfs:comment """Tasks where the goal is to move the prehensile effectors, ie. hands, of an agent so as to achieve some spatial or functional relation with some manipulated object. 

Spatial relations refer to positioning the hands in certain ways relative to the manipulated object, for example nearing or distancing them, or aligning them with some relevant axis.

Functional relations here refer to interactions between the hands and manipulated object which constrain the possible behavior of the object. Examples of functional relations in manipulation are support and kinematic control.

Note that manipulation tasks are usually performed with the intention of moving an object in some way, so there is a large conceptual overlap between Manipulating and Actuating.

However these concepts are nonetheless distinguished in what they \"profile\", ie. what they focus on as particularly important.

Actuating profiles the movement of the object itself.

Manipulating profiles the movement of the hands and the functional relations, such as kinematic control, they establish with the manipulated object.

Note: we employ Manipulating here in its literal, original sense, of using hands for some purpose, and not in the metaphorical sense of exerting psychological pressure on someone.""" ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Markup_Language
SOMA:Markup_Language rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Computer_Language ;
                     rdfs:comment """Markup refers to data included in an electronic document which is distinct from the document's content in that it is typically not included in representations of the document for end users, for example on paper or a computer screen, or in an audio stream. Markup is often used to control the display of the document or to enrich its content to facilitate automated processing. A markup language is a set of rules governing what markup information may be included in a document and how it is combined with the content of the document in a way to facilitate use by humans and computer programs.

From Wikipedia: https://en.wikipedia.org/wiki/Markup_language"""@en ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                     rdfs:label "Markup Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#MassAttribute
SOMA:MassAttribute rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty SOMA:hasMassValue ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:double
                                   ] ;
                   rdfs:comment "The quantity of matter which a body contains, as measured by its acceleration under given force or by the force exerted on it by a gravitational field." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Mass attribute" .


###  http://www.ease-crc.org/ont/SOMA.owl#Masterful
SOMA:Masterful rdf:type owl:Class ;
               rdfs:subClassOf SOMA:DexterityDiagnosis ;
               rdfs:comment "A description of masterful behavior." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Material
SOMA:Material rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Intrinsic ;
              rdfs:comment "The matter from which a thing is made." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#MedicalDiagnosis
SOMA:MedicalDiagnosis rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:FunctionalDiagnosis ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasConstituent> ;
                                        owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organism>
                                      ] ;
                      owl:disjointWith SOMA:TechnicalDiagnosis ;
                      rdfs:comment "A functional diagnosis of an organism." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> ;
                      rdfs:label "Medical diagnosis" .


###  http://www.ease-crc.org/ont/SOMA.owl#Memorizing
SOMA:Memorizing rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Learning ;
                rdfs:comment """An atomic mental task in which an Agent saves some (declarative) information for later retrieval.

Examples include a student learning vocabularies or a computer saving some information to a database."""@en ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                rdfs:label "Memorizing"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#MentalAction
SOMA:MentalAction rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                      <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject>
                                                                    )
                                                      ]
                                  ] ;
                  rdfs:comment """An Event construed as the Agent participant affecting Entities that are representations of actual or potential Entities or Events in the physical world in which the Agent is embodied. These representations are maintained by the Agent participant in the 'Mental action' event.

One could argue Mental actions are all Physical actions, because anything the Agent may use to maintain such representations will be physical things, However, we make this distinction because for Mental actions it is less important to consider the physical support of the representation and how it changes, and more important to track how the information content of the representation changes.""" ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                  rdfs:label "Mental action" .


###  http://www.ease-crc.org/ont/SOMA.owl#MentalTask
SOMA:MentalTask rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                  owl:allValuesFrom SOMA:MentalAction
                                ] ;
                rdfs:comment "A Task classifying some MentalAction, that is, an Action through which an Agent manipulates representations stored in its own cognition." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                rdfs:label "Mental task"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#MeshShape
SOMA:MeshShape rdf:type owl:Class ;
               rdfs:subClassOf SOMA:ShapeRegion ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:hasFilePath ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:hasShapeScale ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange SOMA:array_double
                               ] ;
               rdfs:comment "A solid geometrical figure described in a mesh file." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
               rdfs:label "Mesh shape" .


###  http://www.ease-crc.org/ont/SOMA.owl#MeshShapeData
SOMA:MeshShapeData rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isAbout> ;
                                     owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                   ] ;
                   rdfs:comment "An InformationObject containing data about the geometry of a physical object." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-NEEM.owl> ;
                   rdfs:label "Mesh-shape data"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Message
SOMA:Message rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Item ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTask> ;
                               owl:someValuesFrom SOMA:CommunicationTask
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                               owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationRealization>
                             ] ;
             rdfs:comment """A message is a discrete unit of communication intended by the source for consumption by some recipient or group of recipients (Source: https://en.wikipedia.org/wiki/Message).

Note that the Role Message classifies the Information Realization, not the content."""@en ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
             rdfs:label "Message"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#MetaCognitionEvaluationTopic
SOMA:MetaCognitionEvaluationTopic rdf:type owl:Class ;

                          rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role> ;
                          rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role> ;
                          rdfs:comment "Simple top-level property for relations between two roles." ;
                          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                          rdfs:label "relates to another role" .


###  http://www.ease-crc.org/ont/SOMA.owl#replaces
SOMA:replaces rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf SOMA:after ;
              rdfs:domain SOMA:State ;
              rdfs:range SOMA:State ;
              owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isEventIncludedIn>
                                       SOMA:isTerminalSituationOf
                                       SOMA:hasInitialSituation
                                       <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesEvent>
                                     ) ;
              rdfs:comment "The relation between a State that replaces another, e.g., the state of a bowl of fruits containing some objects is replaced by a new containment state when one object is taken away (in this example, we simplified the relation between the State and its type)."@en ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
              rdfs:label "replaces"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#simultaneous
SOMA:simultaneous rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf SOMA:coOccurs ;
                  rdf:type owl:SymmetricProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                  rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                  SOMA:symbol "=" ;
                  rdfs:comment "`A simultaneous B` means that A strictly starts and ends at the same time instant as B, i.e. their temporal extend is equal." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#startedBy
SOMA:startedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf SOMA:coOccurs ;
               owl:inverseOf SOMA:starts ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
               rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
               SOMA:symbol "si" ;
               rdfs:comment "`A starts B` means that A starts exactly where B starts, and that A strictly ends before B. As in \"I start my day with a coffee\"." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> ;
               rdfs:label "started by"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#starts
SOMA:starts rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf SOMA:coOccurs ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
            rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
            SOMA:symbol "s" ;
            rdfs:comment "`A starts B` means that A starts exactly where B starts, and that A strictly ends before B. As in \"I start my day with a coffee\"." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#supports
SOMA:supports rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf SOMA:interactsWith ,
                                 <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary> ;
              rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ;
              rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ;
              rdfs:comment "A relation between an object (the supporter) and another object (the supportee) where the supporter cancels the effect of gravity on the supportee." ,
                           "Relates a supportee to one of its supporters." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#terminates
SOMA:terminates rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                owl:propertyChainAxiom ( SOMA:causesTransition
                                         SOMA:hasInitialSituation
                                         <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesEvent>
                                       ) ;
                rdfs:comment "The association between an Event that terminates another Event, e.g., the Action of picking an apple from a bowl of fruits terminates the State of containment between the apple and the bowl."@en ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                rdfs:label "terminates"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#transitionsBack
SOMA:transitionsBack rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf SOMA:transitionsFrom ,
                                        SOMA:transitionsTo ;
                     rdfs:domain SOMA:Transient ;
                     rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> ;
                     rdfs:comment "A property which relates a Transient to an Object it both changes from and changes into. This is useful to model objects which, through participation in a process, transform themselves so that an ontological reclassification is necessary, however this transformation is reversible and at the end of the process the objects revert to their previous kind. An example of this is catalysts in chemistry." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "transitions back"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#transitionsFrom
SOMA:transitionsFrom rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary> ;
                     rdfs:domain SOMA:Transient ;
                     rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> ;
                     rdfs:comment "A property which relates a Transient to an Object it changes from. This is useful to model objects which, through participation in a process, transform themselves so that an ontological reclassification is necessary. An example of this is dough undergoing the Maillard reaction through baking." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "transitions from"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#transitionsTo
SOMA:transitionsTo rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary> ;
                   rdfs:domain SOMA:Transient ;
                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> ;
                   rdfs:comment "A property which relates a Transient to an Object it changes into. This is useful to model objects which, through participation in a process, transform themselves so that an ontological reclassification is necessary. An example of this is baked dough eventually becoming bread by completing a baking process." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "transitions to"@en .


###  http://www.ease-crc.org/ont/SOMA.owl/hasExpectedTerminalSituation
<http://www.ease-crc.org/ont/SOMA.owl/hasExpectedTerminalSituation> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf SOMA:hasTerminalSituation ,
                                                                                       <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPostcondition> ;
                                                                    rdfs:domain SOMA:SituationTransition ;
                                                                    rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                                    rdfs:comment "A relation between a Transition and the Situation it is expected to, and does actually, end at. You can assert this relationship when the observed outcome of a transition matches expectations." ;
                                                                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                                                    rdfs:label "has expected terminal situation"@en .


###  http://www.ease-crc.org/ont/SOMA.owl/hasRequiredInitialSituation
<http://www.ease-crc.org/ont/SOMA.owl/hasRequiredInitialSituation> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf SOMA:hasInitialSituation ,
                                                                                      <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPrecondition> ;
                                                                   rdfs:domain SOMA:SituationTransition ;
                                                                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                                   rdfs:comment "A relationship between a Situation x and another Situation y that precedes it, such that without y manifesting, it would be impossible for x to manifest." ;
                                                                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                                                   rdfs:label "has required initial situation"@en .


###  http://www.ease-crc.org/ont/SOMA.owl/manifestsIn
<http://www.ease-crc.org/ont/SOMA.owl/manifestsIn> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesEvent> ;
                                                   rdfs:comment "A relationship indicating that a Situation is realized in an Event that actually happened." ;
                                                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                                   rdfs:label "manifests in" .


###  http://www.ease-crc.org/ont/SOMA.owl/preventedBy
<http://www.ease-crc.org/ont/SOMA.owl/preventedBy> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasSetting> ;
                                                   owl:inverseOf <http://www.ease-crc.org/ont/SOMA.owl/prevents> ;
                                                   rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                   rdfs:comment "A relationship indicating a Situation is prevented by another from manifesting." ;
                                                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                                   rdfs:label "prevented by" .


###  http://www.ease-crc.org/ont/SOMA.owl/prevents
<http://www.ease-crc.org/ont/SOMA.owl/prevents> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor> ;
                                                rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                rdfs:comment "A relationship indicating that a situation does or would prevent another from manifesting. Useful for reasoning about planning (what to do to avoid some bad outcome) and failure recovery (what aspects of the world state prevent continuing the plan?)." ;
                                                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ;
                                                rdfs:label "prevents" .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#executesPlan
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#executesPlan> rdf:type owl:ObjectProperty ;
                                                                          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                          owl:inverseOf <http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isPlanExecutedIn> ;
                                                                          rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                                                                          rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                          rdfs:comment """The relation holding between an Event, and a Plan that is executed by the Event. 

This relationship is used as a primitive which means following the sequence of actions in the plan as much as possible."""@en ;
                                                                          rdfs:label "executes plan"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#hasGoal
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#hasGoal> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                     owl:inverseOf <http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isGoalOf> ;
                                                                     rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                     rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal> ;
                                                                     rdfs:comment "The relation holding between any Agent, and a Goal desired by the Agent."@en ;
                                                                     rdfs:label "has goal"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#hasPlan
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#hasPlan> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                     owl:inverseOf <http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isPlanOf> ;
                                                                     rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                     rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                     rdfs:comment "The relation holding between any Agent, and its Plan to achieve one of its desired Goals."@en ;
                                                                     rdfs:label "has plan"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isBetterPlanThan
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isBetterPlanThan> rdf:type owl:ObjectProperty ;
                                                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                              owl:inverseOf <http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isWorsePlanThan> ;
                                                                              rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                              rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                              rdfs:label "is better plan than"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isGoalOf
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isGoalOf> rdf:type owl:ObjectProperty ;
                                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                      rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal> ;
                                                                      rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                      rdfs:comment "The relation holding between a Goal, and an Agent that desires that Goal."@en ;
                                                                      rdfs:label "is goal of"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isPlanExecutedIn
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isPlanExecutedIn> rdf:type owl:ObjectProperty ;
                                                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                              rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                              rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                                                                              rdfs:comment "The relation holding between a Plan, and an Event that executes that plan."@en ;
                                                                              rdfs:label "is plan executed in"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isPlanOf
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isPlanOf> rdf:type owl:ObjectProperty ;
                                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                      rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                      rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                      rdfs:comment "The relation holding between a Plan, and the Agent that has that Plan to achieve one of its desired Goals."@en ;
                                                                      rdfs:label "is plan of"@en .


###  http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isWorsePlanThan
<http://www.iri.upc.edu/groups/perception/OCRA/ont/ocra.owl#isWorsePlanThan> rdf:type owl:ObjectProperty ;
                                                                             rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                             rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                             rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                                                             rdfs:label "is worse plan than"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsFor
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsFor> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                owl:inverseOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsThrough> ;
                                                                rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialAgent> ;
                                                                rdfs:comment "The relation holding between any Agent, and a SocialAgent. In principle, a SocialAgent requires at least one PhysicalAgent in order to act, but this dependency can be 'delegated'; e.g. a university can be acted for by a department, which on its turm is acted for by physical agents." ;
                                                                rdfs:isDefinedBy <http://www.ease-crc.org/ont/DUL.owl> ,
                                                                                 <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                rdfs:label "acts for"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsThrough
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsThrough> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                    rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialAgent> ;
                                                                    rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                    rdfs:comment "The relation holding between a PhysicalAgent and a SocialAgent. In principle, a SocialAgent requires at least one PhysicalAgent in order to act, but this dependency can be 'delegated', e.g. a university can be acted for by a department, which is acted for by physical agents. AKA isActedBy" ;
                                                                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/DUL.owl> ,
                                                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                    rdfs:label "acts through"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> rdf:type owl:ObjectProperty ;
                                                                       owl:inverseOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                       rdf:type owl:SymmetricProperty ,
                                                                                owl:TransitiveProperty ;
                                                                       rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ;
                                                                       rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ;
                                                                       rdfs:comment """A catch-all object property, useful for alignment and querying purposes.
It is declared as both transitive and symmetric, in order to reason an a maximal closure of associations between individuals.""" ;
                                                                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/DUL.owl> ,
                                                                                        <http://www.ease-crc.org/ont/SOMA-PROC.owl> ,
                                                                                        <http://www.ease-crc.org/ont/SOMA.owl> ,
                                                                                        <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                       rdfs:label "associated with"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#characterizes
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#characterizes> rdf:type owl:ObjectProperty ;
                                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                      owl:inverseOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isCharacterizedBy> ;
                                                                      rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept> ;
                                                                      rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Collection> ;
                                                                      rdfs:comment """A relation between concepts and collections, where a Concept is said to characterize a Collection; it corresponds to a link between the (reified) intensional and extensional interpretations of a _proper subset of_ a (reified) class. This is different from covers, because it refers to an interpretation the entire reified class.
E.g. the collection of vintage saxophones is characterized by the Concept 'manufactured by hand', while it gets covered by the Concept 'Saxophone' with the Parameter 'Vintage'.""" ;
                                                                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/DUL.owl> ,
                                                                                       <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                      rdfs:label "characterizes"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                   owl:inverseOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy> ;
                                                                   rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept> ;
                                                                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ;
                                                                   rdfs:comment "A relation between a Concept and an Entity, e.g. the Role 'student' classifies a Person 'John'." ;
                                                                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/DUL.owl> ,
                                                                                    <http://www.ease-crc.org/ont/SOMA-PROC.owl> ,
                                                                                    <http://www.ease-crc.org/ont/SOMA-STATE.owl> ,
                                                                                    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                   rdfs:label "classifies"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#conceptualizes
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#conceptualizes> rdf:type owl:ObjectProperty ;
                                                                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                       owl:inverseOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isConceptualizedBy> ;
                                                                       rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                                                                       rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject> ;
                                                                       rdfs:comment """A relation stating that an Agent is internally representing a SocialObject: situations, descriptions, concepts, etc. E.g., 'John believes in the conspiracy theory'; 'Niels Bohr created the solar-system metaphor for the atomic theory'; 'Jacques assumes all swans are white'; 'the task force members share the attack plan'.
Conceptualizations can be distinguished into different forms, primarily based on the type of SocialObject that is conceptualized. Descriptions and concepts can be 'assumed', situations can be 'believed' or 'known', plans can be 'adopted', etc. (see ontology: http://www.ontologydesignpatterns.org/ont/dul/Conceptualization.owl.""" ;
                                                                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/DUL.owl> ,
                                                                                        <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                       rdfs:label "conceptualizes"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#concretelyExpresses
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#concretelyExpresses> rdf:type owl:ObjectProperty ;
                                                                            rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;

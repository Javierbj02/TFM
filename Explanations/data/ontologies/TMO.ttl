                                        owl:someValuesFrom [ owl:intersectionOf ( SOMA:MovedObject
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                                                                    owl:allValuesFrom SOMA:PrehensileEffector
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                      ] ;
                      rdfs:comment "A motion of a prehensile effector." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                      rdfs:label "Prehensile motion" .



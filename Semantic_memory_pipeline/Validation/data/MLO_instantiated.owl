  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The execution state of a cancelled activity.</rdfs:comment>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ExecutionState_Failed">
  <rdf:type rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ExecutionStateRegion"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-ACT.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The execution state of a failed activity.</rdfs:comment>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ExecutionState_Paused">
  <rdf:type rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ExecutionStateRegion"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-ACT.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The execution state of a paused activity.</rdfs:comment>
